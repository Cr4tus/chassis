{
  "name": "chassis",
  "version": "1.0.0",
  "description": "Utilities and types package for Antify",
  "type": "module",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./dist/*.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean --external mongodb --external zod",
    "dev": "tsup src/index.ts --format esm --watch --dts --external mongodb --external zod",
    "prepare": "pnpm run build"
  },
  "keywords": [],
  "author": "Cr4tus",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "24.9.1",
    "@vitest/coverage-v8": "4.0.1",
    "typescript": "^5.0.0",
    "encore.dev": "^1.53.4",
    "mongodb": "^6.20.0",
    "vitest": "4.0.1",
    "zod": "^4.1.12",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "encore.dev": "^1.53.4",
    "mongodb": "^6.20.0",
    "vitest": "^4.0.1",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.28.0"
}